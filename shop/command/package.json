{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "openapi-codegen": "npx openapi-typescript 'openapi/**/*.yaml' --output schemas/",
    "asyncapi-codegen": "npx openapi-typescript 'asyncapi/**/*.yaml' --output schemas/",
    "prisma": "npx dotenv -e .env.local -- npx prisma",
    "test": "find ./app -name '*.spec.ts' -exec npx dotenv -e .env.local -- node --import tsx --test {} \\;",
    "domain-test": "find ./lib/domain -name '*.spec.ts' -exec npx dotenv -e .env.local -- node --import tsx --test {} \\;",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "dotenv-cli": "^7.3.0",
    "jsonwebtoken": "^9.0.2",
    "next": "14.0.3",
    "openapi-typescript": "^6.7.1",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "prisma": "^5.6.0",
    "tsx": "^4.1.3",
    "typescript": "^5"
  }
}
